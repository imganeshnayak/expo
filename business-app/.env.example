# UMA Business/Merchant App - Environment Variables
# Copy this file to .env and fill in your actual values

# Backend API Configuration
EXPO_PUBLIC_API_URL=https://your-api-url.com

# Supabase Configuration (if using Supabase)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Analytics Configuration (optional)
EXPO_PUBLIC_ANALYTICS_KEY=your-analytics-key-here

# Push Notifications (optional)
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your-push-key-here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_APP_VERSION=1.0.0

# Business App Specific
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CAMPAIGNS=true
EXPO_PUBLIC_ENABLE_CRM=true
EXPO_PUBLIC_ENABLE_AI_RECOMMENDATIONS=true

# API Endpoints (if different from base URL)
EXPO_PUBLIC_CAMPAIGNS_API=/api/campaigns
EXPO_PUBLIC_ANALYTICS_API=/api/analytics
EXPO_PUBLIC_CRM_API=/api/crm
EXPO_PUBLIC_CUSTOMERS_API=/api/customers
