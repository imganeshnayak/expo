# Environment Configuration for Utopia App

# API Backend Configuration
EXPO_PUBLIC_API_URL=http://192.168.1.3:5000
EXPO_PUBLIC_API_TIMEOUT=30000

# Environment Configuration for ONDC Integration

# ONDC Gateway Configuration
ONDC_GATEWAY_URL=https://staging.gateway.proteantech.in
ONDC_SUBSCRIBER_ID=uma-rider-app
ONDC_SUBSCRIBER_URL=https://uma-app.com/ondc/webhook
ONDC_REGISTRY_URL=https://staging.registry.ondc.org

# Authentication
ONDC_SIGNING_PRIVATE_KEY=your_private_key_here
ONDC_SIGNING_PUBLIC_KEY=your_public_key_here
ONDC_ENCRYPTION_PRIVATE_KEY=your_encryption_private_key_here
ONDC_ENCRYPTION_PUBLIC_KEY=your_encryption_public_key_here

# App Configuration
ONDC_COUNTRY_CODE=IND
ONDC_CITY_CODE=std:080
ONDC_DOMAIN=ONDC:TRV10  # Mobility domain

# Provider Specific (Namma Yatri)
NAMMA_YATRI_PROVIDER_ID=nammayatri.in
NAMMA_YATRI_SUBSCRIBER_ID=nammayatri.in/ondc

# UMA App Identifiers
UMA_TRANSACTION_ID_PREFIX=UMA-TXN-
UMA_MESSAGE_ID_PREFIX=UMA-MSG-

# Staging vs Production
ONDC_ENVIRONMENT=staging  # or 'production'
